# Anthropic Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Yelp Fusion API (PRIMARY DATA SOURCE)
# Get your key at: https://www.yelp.com/developers/v3/manage_app
YELP_API_KEY=your_yelp_api_key_here

# Bright Data Proxy Configuration (OPTIONAL - currently not implemented)
BRIGHT_DATA_USERNAME=your_username
BRIGHT_DATA_PASSWORD=your_password
BRIGHT_DATA_ZONE=your_zone
BRIGHT_DATA_PROXY=brd.superproxy.io:22225

# Fetch.ai uAgents Configuration
UAGENTS_WALLET_MNEMONIC=your_wallet_mnemonic_here
UAGENTS_NETWORK=alpha

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# ChromaDB Configuration
CHROMA_PATH=./chroma_data
CHROMA_HOST=localhost
CHROMA_PORT=8000

# Application Configuration
APP_ENV=local
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000
CORS_ORIGINS=http://localhost:3000

# Logging
LOG_LEVEL=info
